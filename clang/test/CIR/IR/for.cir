// RUN: cir-opt %s --verify-roundtrip | FileCheck %s

cir.func @testPrintingParsing(%arg0 : !cir.bool) {
  cir.for : cond {
    cir.condition(%arg0)
  } body {
    cir.yield
  } step {
    cir.yield
  }
  cir.return
}

// CHECK: @testPrintingParsing
// CHECK: cir.for : cond {
// CHECK:   cir.condition(%arg0)
// CHECK: } body {
// CHECK:   cir.yield
// CHECK: } step {
// CHECK:   cir.yield
// CHECK: }
