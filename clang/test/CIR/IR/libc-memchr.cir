// RUN: cir-opt %s

!voidptr = !cir.ptr<!cir.void>
!u32i = !cir.int<u, 32>
!u64i = !cir.int<u, 64>
module {
  cir.func @f(%src : !voidptr, %pattern : !u32i, %len : !u64i) -> !voidptr {
    %ptr = cir.libc.memchr(%src : !voidptr, %pattern, %len : !u64i) -> !voidptr
    cir.return %ptr : !voidptr
  }
}
