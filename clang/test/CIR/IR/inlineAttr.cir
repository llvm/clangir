// RUN: cir-opt %s | FileCheck %s -check-prefix=CIR
// RUN: cir-opt %s -cir-to-llvm -o - | FileCheck %s -check-prefix=MLIR

#fn_attr = #cir<extra({inline = #cir.inline<no>})>

module {
  cir.func @l0() extra(#fn_attr)  {
    cir.return
  }
}

