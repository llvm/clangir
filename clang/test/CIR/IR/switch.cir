// RUN: cir-tool %s | FileCheck %s

cir.func @s0() {
  %1 = cir.cst(2 : i32) : i32
  cir.switch (%1 : i32) [
    case (default) {
      cir.return
    },
    case (equal, 3) {
      cir.yield fallthrough
    },
    case (anyof, [6, 7, 8] : i32) {
      cir.yield break
    },
    case (equal, 5 : i32) {
      cir.yield
    }
  ]
  cir.return
}

