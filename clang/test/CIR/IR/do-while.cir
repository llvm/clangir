// RUN: cir-opt %s --verify-roundtrip | FileCheck %s

cir.func @testPrintingAndParsing (%arg0 : !cir.bool) -> !cir.void {
  cir.do {
    cir.yield
  } while {
    cir.condition(%arg0)
  }
  cir.return
}

// CHECK: testPrintingAndParsing
