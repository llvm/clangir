// RUN: cir-opt %s -cir-to-mlir | FileCheck %s -check-prefix=MLIR

!s32i = !cir.int<s, 32>
module {
  cir.func @br() -> !s32i {
    %0 = cir.const #cir.int<2> : !s32i
    cir.br ^bb1(%0 : !s32i)
  ^bb1(%v1 : !s32i):
    cir.return %v1 : !s32i
  // MLIR: %[[A:.*]] = arith.constant 2 : i32
  // MLIR-NEXT:  cf.br ^bb1(%[[A]] : i32)
  // MLIR-NEXT:^bb1(%[[V1:.*]]: i32):
  // MLIR-NEXT:  return %[[V1]] : i32
  }
}