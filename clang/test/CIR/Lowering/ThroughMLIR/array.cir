// RUN: cir-opt %s -cir-to-mlir -o - | FileCheck %s -check-prefix=MLIR

module {
  cir.func @foo() {
    %0 = cir.alloca !cir.array<i32 x 10>, cir.ptr <!cir.array<i32 x 10>>, ["a"] {alignment = 16 : i64}
    cir.return
  }
}

