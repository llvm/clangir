// RUN: cir-tool %s -cir-to-memref -o %t.mlir
// RUN: FileCheck --input-file=%t.mlir %s
// XFAIL: *

module {
  func.func @foo() {
    %0 = cir.alloca i32, cir.ptr <i32>, [cinit]
    %1 = cir.cst(1 : i32) : i32
    cir.return
  }
}

//      CHECK: module  {
// CHECK-NEXT:   func.func @foo() {
// CHECK-NEXT:     %0 = memref.alloc() : memref<i32>
// CHECK-NEXT:     {{.*}}arith.constant 1 : i32
// CHECK-NEXT:     cir.return
// CHECK-NEXT:   }
// CHECK-NEXT: }
